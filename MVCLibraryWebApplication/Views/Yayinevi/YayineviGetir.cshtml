@model MVCLibraryWebApplication.Models.Entity.TBL_YAYINEVI

@{
    ViewBag.Title = "YayineviGetir";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Yayınevi Güncelle</h2>

<br />

@using (Html.BeginForm("YayineviGuncelle", "Yayinevi", FormMethod.Post))
{
    <div class="form-group">
        @Html.LabelFor(k => k.ID)
        @Html.TextBoxFor(k => k.ID, new { @class = "form-control" })
        <br />
        @Html.LabelFor(k => k.AD)
        @Html.TextBoxFor(k => k.AD, new { @class = "form-control" })
    </div>

    <div>
        <button class="btn btn-warning">Güncelle</button>
    </div>
}